{
  "name": "eslint-config-badboyku",
  "description": "Shared eslint+prettier configuration with react/jest/typescript support.",
  "version": "1.0.31",
  "author": "badboyku",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/badboyku/eslint-config-badboyku.git"
  },
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "prettier --check \"**/*.{css,scss,json}\"",
    "prettier:fix": "prettier --write \"**/*.{css,scss,json}\""
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  }
}
